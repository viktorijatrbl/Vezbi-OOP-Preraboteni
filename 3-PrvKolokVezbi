
int main() {
    char ime[100], destinacija[100];
    int n;
    bool velosiped;
    int klasa;
    int maxv;

    cin >> destinacija >> maxv;
    cin >> n;

    Voz v(destinacija, maxv);

    for (int i = 0; i < n; i++) {
        cin >> ime >> klasa >> velosiped;
        Patnik p(ime, klasa, velosiped);
        v.addPassenger(p);
    }

    v.print();
    v.patniciNemaMesto();

    return 0;
}
